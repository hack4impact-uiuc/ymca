  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint --fix src --ext js,jsx",
    "lint:check": "eslint src --ext js,jsx",
    "format": "prettier --write \"./**/*.{js,jsx,json,md}\"",
    "format:check": "prettier --check \"./**/*.{js,jsx,json,md}\"",
